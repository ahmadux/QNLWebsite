<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>QNL Website</display-name>
  <welcome-file-list>
    <welcome-file></welcome-file>
  </welcome-file-list>
  <listener>
    <listener-class>
  			com.qnl.management.Initializer
  		</listener-class>
  </listener>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.qnl.management.Login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PageSetupController</servlet-name>
    <servlet-class>com.qnl.management.PageSetupController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Reloader</servlet-name>
    <servlet-class>com.qnl.management.Reloader</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ExternalServices</servlet-name>
    <servlet-class>com.qnl.management.ExternalServices</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AjaxToDB</servlet-name>
    <servlet-class>com.qnl.management.AjaxToDB</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FileManagement</servlet-name>
    <servlet-class>com.qnl.management.FileManagement</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FileUpload</servlet-name>
    <servlet-class>com.qnl.management.FileUpload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PageSetupController</servlet-name>
    <url-pattern>/PageSetupController.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ExternalServices</servlet-name>
    <url-pattern>/ExternalServices.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AjaxToDB</servlet-name>
    <url-pattern>/AjaxToDB.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Reloader</servlet-name>
    <url-pattern>/Reloader.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FileManagement</servlet-name>
    <url-pattern>/FileManagement.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Login.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FileUpload</servlet-name>
    <url-pattern>/FileUpload.do</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>URLFilter</filter-name>
    <filter-class>
        com.qnl.management.URLFilter
    </filter-class>
  </filter>
  <filter-mapping>
    <filter-name>URLFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <error-page>
    <location>/error.jsp</location>
  </error-page>  
</web-app>